<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">





    <include layout="@layout/layout_week_num" />


    <com.necer.ncalendar.calendar.NMonthCalendar
        android:id="@+id/monthCalendar"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:visibility="visible"></com.necer.ncalendar.calendar.NMonthCalendar>


    <com.necer.ncalendar.calendar.NWeekCalendar
        android:id="@+id/weekCalendar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:visibility="invisible"></com.necer.ncalendar.calendar.NWeekCalendar>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转"
        android:onClick="aaa"/>

</LinearLayout>